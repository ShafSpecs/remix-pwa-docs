    "@octokit/request": "^6.2.3",
    "@remix-pwa/sw": "^0.3.6",
    "@remix-run/node": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@remix-run/vercel": "^1.18.1",
    "@vercel/analytics": "^1.0.1",
    "@vercel/node": "^2.14.4",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.3",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "mdx-bundler": "^9.2.1",
    "node-persist": "^3.1.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prism-react-renderer": "^2.0.4",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-spring": "^9.7.1",
    "redent": "^4.0.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-prism": "^2.2.2",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark-emoji": "^3.1.1",
    "remark-gfm": "^3.0.1",
    "remix-typedjson": "^0.1.7",
    "remix-utils": "^6.0.0",
    "tailwindcss-font-inter": "^3.0.1",

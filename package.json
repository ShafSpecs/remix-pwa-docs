    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "prettier": "^2.8.4",
